{
  "manifest_version": 2,

  "name": "hyperdragons go",
  "description": "hyperdragons 变异提醒",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "permissions": [
    "http://kovan.hyperdragons.com/","notifications","background","tabs"
  ],

  "icons": { 
                
    "48": "icon.png"           
     
  }, 
  "background":
    {
      
        "page":"popup.html"
        
    },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
 

}